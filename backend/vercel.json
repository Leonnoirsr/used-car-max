{
  "version": 2,
  "name": "used-car-max",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://used-car-l5tv6hgad-leonnoirsr.vercel.app/",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}